{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": [
      "es2019"
    ],
    "outDir": "dist",
    "rootDir": ".",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}